{
  "name": "auction",
  "version": "1.0.0",
  "description": "Auction microservice",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "@sentry/node": "^6.14.1",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "inversify": "^5.1.1",
    "joi": "^17.4.2",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3",
    "uuid": "^9.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "typeorm": "^0.2.22",
    "module-alias": "^2.2.2",
    "ts-custom-error": "^3.2.2",
    "bluebird": "^3.7.2",
    "tslib": "^2.4.0",
    "password-hash": "^1.2.2",
    "tsoa": "^4.1.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/debug": "^4.1.7",
    "@types/password-hash": "^1.2.21",
    "@types/lodash.get": "^4.4.6",
    "@types/mongoose-sequence": "^3.0.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.9.1",
    "pg": "^8.8.0",
    "pg-promise": "^10.10.1",
    "pg-query-stream": "^4.0.0",
    "tslib": "^2.4.0"
  },
  "scripts": {
    "init": "tsc --init",
    "swagger-gen": "tsoa spec-and-routes",
    "tsoa": "tsoa",
    "start": "tsnd -r tsconfig-paths/register -r dotenv/config --watch .env,./src/**/*.ts --respawn src/index.ts",
    "build": "rm -rf dist && tsc",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js --config src/libs/typeorm/ormconfig.ts migration:run -c default",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js --config src/libs/typeorm/ormconfig.ts migration:revert -c default",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js --config src/libs/typeorm/ormconfig.ts schema:drop -c default",
    "test:integration": "jest --config=./tests/integration/jest.config.ts --verbose --runInBand tests/integration",
    "test:unit": "jest --config=./tests/unit/jest.config.ts --verbose --runInBand ./tests/unit",
    "lint": "eslint --ignore-path .eslintignore --ext .ts,.js .",
    "format": "prettier --write '*.{ts,js,json}' './src/**/*.{ts,js,json}' './tests/**/*.{ts,js,json}'"
  },
  "author": "Ahmad",
  "license": "UNLICENSED",
  "private": true
}
